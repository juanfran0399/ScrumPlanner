#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/francisco/Desktop/Development/ScrumPlanner/node_modules/.pnpm/ts-standard@12.0.2_typescript@5.5.4/node_modules/ts-standard/node_modules:/home/francisco/Desktop/Development/ScrumPlanner/node_modules/.pnpm/ts-standard@12.0.2_typescript@5.5.4/node_modules:/home/francisco/Desktop/Development/ScrumPlanner/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/francisco/Desktop/Development/ScrumPlanner/node_modules/.pnpm/ts-standard@12.0.2_typescript@5.5.4/node_modules/ts-standard/node_modules:/home/francisco/Desktop/Development/ScrumPlanner/node_modules/.pnpm/ts-standard@12.0.2_typescript@5.5.4/node_modules:/home/francisco/Desktop/Development/ScrumPlanner/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
